[user]
name = XXX
email = XXX@example.com

[alias]
ci = commit
br = branch
co = checkout
sw = switch
re = restore
st = status
df = diff
pl = pull

b = branch -vv
bb = branch -avv

f = fetch -v
ff = fetch -v --all --prune

all = log --oneline --decorate --graph --branches --tags --remotes

[pager]
log  = diff-highlight | less
show = diff-highlight | less
diff = diff-highlight | less
